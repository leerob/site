  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leerob/leerob.io.git"
  },
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint . --ext .json --ext .js",
